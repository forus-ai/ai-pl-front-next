{
  "name": "nextjs-with-typescript",
  "version": "0.1.0",
  "private": true,
  "description": "Simple project that shows the usage of Next.js with TypeScript",
  "author": "forus-ai",
  "scripts": {
    "dev": "yarn run gen:theme-typings && open http://localhost:3000 & next dev ",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "gen:theme-typings": "chakra-cli tokens styles/theme",
    "postinstall": "yarn run gen:theme-typings",
    "test": "jest --watch",
    "lint": "tsc --noEmit && eslint --fix ./ && pretty-quick --staged",
    "commit": "cz"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@hookform/resolvers": "^2.8.2",
    "@types/yup": "^0.29.13",
    "axios": "^0.24.0",
    "dayjs": "^1.10.6",
    "framer-motion": "^4",
    "lodash": "^4.17.21",
    "next": "^11.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.18.0",
    "react-query": "^3.19.1",
    "react-select": "^5.2.0",
    "slick-carousel": "^1.8.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.5.0",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/node": "^16.4.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-jest": "^27.2.4",
    "commitizen": "^4.2.4",
    "cypress": "^8.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "git-cz": "^4.8.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.4",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "license": "ISC"
}
